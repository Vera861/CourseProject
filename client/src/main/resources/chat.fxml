<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox xmlns:fx="http://javafx.com/fxml/1" alignment="center" xmlns="http://javafx.com/javafx/8"
      fx:controller="ChatController">
    <children>

        <TextField fx:id="statusBar" VBox.vgrow="ALWAYS">
            <VBox.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
            </VBox.margin>
        </TextField>
        <TableView fx:id="tableFile" VBox.vgrow="ALWAYS">
            <VBox.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
            </VBox.margin>
        </TableView>
        <HBox alignment="center" spacing="15">
            <children>
                <Button layoutX="51.0" layoutY="482.0" mnemonicParsing="false" onAction="#send" prefHeight="25.0"
                        prefWidth="73.0" text="Copy"/>
                <Button layoutX="154.0" layoutY="482.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="73.0"
                        text="Move"/>
                <Button layoutX="259.0" layoutY="482.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="67.0"
                        text="Delete"/>
            </children>
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
            </padding>
        </HBox>
    </children>
</VBox>
